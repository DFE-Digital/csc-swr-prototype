<div>
  Please upload documents for your case
  <%= form_with model: @case do |f| %>
    <%= f.label :case_name %>
    <br><br>
    <%= f.text_field :name %>
    <br><br>
    <%= f.label :image_upload %>
    <br><br>
    <%= f.file_field :image %>
    <br><br>
    <%= f.submit :Submit %>
  <% end %>
</div>